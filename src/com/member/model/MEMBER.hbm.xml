<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

	<class name="com.member.model.MemberVO" table="MEMBER">

	<id name="mem_id" column="mem_id">
		<generator class="generator.Mem_Generator" /> 
	</id>
	<property name="mem_pwd" column="mem_pwd"/>
	<property name="mem_name" column="mem_name"/>
	<property name="mem_idnum" column="mem_idnum"/>
	<property name="mem_bday" column="mem_bday"/>	
	<property name="mem_phone" column="mem_phone"/>
	<property name="mem_addr" column="mem_addr"/>
	<property name="mem_mail" column="mem_mail"/>	
	<property name="mem_due" column="mem_due"/>
	<property name="key_id" column="key_id"/>
	<property name="key_date" column="key_date"/>	
	<property name="mem_um" column="mem_um"/>					
	</class>
</hibernate-mapping>