<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.shiftreport.model.ShiftreVO" table="dbo.SHIFTREPORT">
		<composite-id>
			<key-property name="Date" column="Date" />
			<key-property name="shift" column="shift" />
<!-- 			    <generator class=null></generator>  -->
			
		</composite-id>

		<property name="emp_id" column="emp_id" />
		<property name="cash" column="cash" />
		<property name="coupon" column="coupon" />
		<property name="discount" column="discount" />
		<property name="coins" column="coins" />
		<property name="deal_sum" column="deal_sum" />
		<property name="deal_cost" column="deal_cost" />
		<property name="deal_profit" column="deal_profit" />
		<property name="deal_num" column="deal_num" />
		<property name="shift_sum" column="shift_sum" />
		<property name="real_cash" column="real_cash" />
		<property name="real_coupon" column="real_coupon" />

	</class>
</hibernate-mapping>