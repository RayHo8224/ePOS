<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

	<class name="com.promoting.model.PromotingVO" table="PROMOTING">
	
	<composite-id>
		<key-property name="pro_prod_id" column="pro_prod_id" />
		<key-property name="pro_begin" column="pro_begin" />	
	</composite-id>

	<property name="pro_prod_name" column="pro_prod_name"/>	
	<property name="pro_end" column="pro_end"/>
	<property name="pro_neirong" column="pro_neirong"/>
				
	</class>

</hibernate-mapping>